# n8n webhook URL (private)
N8N_JOBS_ENDPOINT="https://your-n8n-host/webhook/your-jobs-webhook"

# Header auth config for n8n webhook
N8N_AUTH_HEADER_NAME="x-n8n-auth"
N8N_AUTH_HEADER_VALUE="replace-with-long-random-secret"

# Secret for on-demand revalidation (n8n calls /api/revalidate?secret=<this>)
REVALIDATE_SECRET="replace-with-long-random-secret"
